  "devDependencies": {
    "concurrently": "6.0.2",
    "react-scripts": "^4.0.3"
  },
  "dependencies": {
    "autoprefixer-stylus": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "firebase": "^8.4.1",
    "re-base": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "stylus": "^0.54.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "sylus -u atuoprefixer-stylus ./src/css/style.styl -o ./src/css/styles.css",
    "styles:watch": "stylus -u sutoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
